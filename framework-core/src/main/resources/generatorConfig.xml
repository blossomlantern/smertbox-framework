<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
<classPathEntry location="F:/SmertBox/jar/db_jar/mysql-connector-java-5.1.39.jar"/>

  <context id="MysqlContext" targetRuntime="MyBatis3">
    <commentGenerator>
        <property name="suppressDate" value="true"/>  
        <!-- 是否去除自动生成的注释 true：是 ： false:否 -->  
        <property name="suppressAllComments" value="true"/>  
    </commentGenerator>
    <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/smertbox" userId="smertbox" password="smertbox" />
    
    
    <javaModelGenerator targetPackage="com.smertbox.system.quartz.model" targetProject="framework-core\src\main\java" >
    	<property name="trimStrings" value="true" />
    </javaModelGenerator>
    <sqlMapGenerator targetPackage="com.smertbox.system.quartz.mapper" targetProject="framework-core\src\main\java" />
    <javaClientGenerator targetPackage="com.smertbox.system.quartz.mapper" targetProject="framework-core\src\main\java" type="XMLMAPPER" />
      
    <!-- 生成全部表 -->
    <!-- <table tableName="%" >
      <columnOverride column="id" property="Mysql" />
    </table> -->
    
    <!-- 生成单表 -->
    <table tableName="qrtz_job_log" domainObjectName="QrtzJobLog"/>  
  </context>
</generatorConfiguration>